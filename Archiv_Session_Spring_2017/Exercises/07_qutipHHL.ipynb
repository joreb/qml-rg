    "deletable": true,
    "editable": true,
    "scrolled": false
   },
   "outputs": [],
   "source": [
    "fsevls, fsevcs = finalstate.eigenstates()\n",
    "x = math.sqrt(fsevls.max()) * fsevcs[fsevls.argmax()]\n",
    "x"
   ]
